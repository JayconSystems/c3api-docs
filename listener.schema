{
    "type": "object",
    "$schema": "http://json-schema.org/draft-03/schema",
    "id": "http://api.c3wireless.com",
    "required": true,
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "required": false
        },
	"owner_id": {
	    "type": "integer",
	    "required": false
	},
	"last_seen": {
	    "type": "timestamp",
	    "required": false
	},
	"state": {
	    "type": "string",
	    "required": false
	},
	"name": {
	    "type": "string",
	    "required": true
	},
	"mac": {
	    "type": "string",
	    "required": true,
	    "minLength": 12,
	    "maxLength": 12
	},
	"description": {
	    "type": "string",
	    "required": false
	},
	"geom": {
	    "type": "string",
	    "required": false
	},
	"zone_id": {
	    "type": "integer",
	    "required": false
	}
    }
}
